//Create a function called takeName passing the parameter "name"
function takeName(name){
	var name_n = name;
	//Generating a random number which in between 5-16
	var random = Math.floor(Math.random() * (16 - 5 + 1) + 5);
	//concatinating the name and the random number
	contName = name_n+random;
	console.log(contName);
	return contName;
	//takeName function will return concatinated form of name and random number
}